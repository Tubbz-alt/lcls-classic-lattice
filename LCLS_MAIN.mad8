! wiggler modeled as R-matrix to include vertical natural focusing

  Kwig  := 50.0                            !wiggler parameter [m^-1]
  lamw  :=  0.34                           !wiggler period [m]
  Lwig  :=  2.38                           !wiggler length [m]
  kqwig := (2*pi*Kwig/lamw/sqrt(2)/gamf)^2 !natural vertical focusing "k" [m^-2]

  WIGXL : MATRIX, &
    L       = Lwig, &
    RM(1,1) = 1.0,  &
    RM(1,2) = Lwig, &
    RM(2,1) = 0.0,  &
    RM(2,2) = 1.0,  &
    RM(3,3) = cos(Lwig*sqrt(kqwig)),  &
    RM(3,4) = sin(Lwig*sqrt(kqwig))/sqrt(kqwig), &
    RM(4,3) = -sin(Lwig*sqrt(kqwig))*sqrt(kqwig),  &
    RM(4,4) = cos(Lwig*sqrt(kqwig))

